}

#home_page {
	background: #fff;
}

#inventory_header {
	background: rgb(0, 159, 161)
}

#inventory_header QPushButton {
	background: rgb(251, 254, 255);
	color: rgb(0, 159, 161);
	font-size: 15pt;;
	width: 150px;
	height: 40px;
	margin: 0;
}

/* combo box */

#inventory_type_input {
	background: rgb(251, 254, 255);
	color: rgb(0, 159, 161);
	height: 40px;
	border-radius: 5px;
	font-size: 18pt;
	padding-left: 10px;
}

#inventory_type_input::drop-down {
	border: 0px;
}

#inventory_type_input::down-arrow {
	image: url(:/icon/images/down-arrow.ico);
	width: 30px;
	height: 30px;
	margin-right: 20px
}

#inventory_type_input:on {
	border: 2px solid rgba(0, 159, 161, 10%);
}

#inventory_type_input QListView{
	font-size: 15pt;
	border: 1px solid rgba(0, 0, 0, 10%);
	padding: 3px;
	background-color: rgb(251, 254, 255);
	outline: 0px;
}

#inventory_type_input QListView::item {
	padding-left: 10px;
	background-color: rgb(251, 254, 255);
}

#inventory_type_input QListView::item:hover {
	background: rgb(0, 159, 161);
	color: rgb(251, 254, 255);
}

#inventory_type_input QListView::item:selected {
	background: rgb(0, 159, 161);
	color: rgb(251, 254, 255);
}








