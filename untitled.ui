	background-size: cover;
}

#login_page QLabel {
	margin-left: 40px;
	font-size: 15pt
}

#login_page QLineEdit, QPushButton {
	height: 70px;
	margin-left: 40px;
	margin-right: 40px;
	border-radius: 5px;
}

#login_page QLineEdit {
	background: rgb(255, 255, 255);
	padding-left: 20px;
	padding-right: 20px;
	font-size: 18pt;
	color: black;
}

#login_page QPushButton {
	background: rgb(0, 159, 161);
	font-size: 23pt;
	margin-top: 40px;
}

#header {
	background: rgb(52, 62, 162);
}

#mic_logo {
	max-width: 75px;
	max-height: 75px;
}

#labify_logo {
	margin-left: 20px;
	max-width: 100px;
	min-height: 95px;
}

#header QPushButton {
	background: rgb(255, 255, 255);
	margin: 0;
	width: 100px;
	margin-top: 10px;
	margin-bottom: 10px;
	border-radius: 3px;
}

#create_user_button {
	color: rgb(0, 159, 161);
}

#logout_button {
	color: rgb(224, 27, 36)
}

#header_username_label {
	background: #fff;
	color: rgb(52, 62, 162);
	font-size: 18pt;
	padding: 5px 40px 5px 40px;
	border-radius: 5px;
}

#footer {
	background: rgb(0, 159, 161);
}

#footer QLabel {
	margin: 0;
}

#create_user_page {
	background: #fff;
}

#create_user_form QLabel {
	color: rgb(52, 62, 162);
	font-size: 15pt;
	max-height: 20px;
	margin-top: 10px;
	padding-top:30px
}

#create_user_form QLineEdit {
	border: 1px solid;
	min-height: 80px;
	border-radius: 5px;
	border-color: rgb(52, 62, 162);
	color: black;
	padding-left: 10px;
	padding-right: 10px;
}

#create_account_button {
	background: rgb(0, 159, 161);
	font-size: 25pt;
	margin: 0;
	margin-right: 40px;
}

#cancel_button {
	border: 1px solid;
	border-color: rgb(224, 27, 36);
